{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "portfolio",
  "compatibility_date": "2025-09-02",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "main": "@tanstack/react-start/server-entry",
  "assets": {
    "directory": "public",
    "binding": "ASSETS"
  },
  "vars": {
    "NODE_ENV": "production"
  },
  "routes": [
    {
      "pattern": "johnfulkerson.com",
      "custom_domain": true
    }
  ]
}